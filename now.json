{
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/static/$1"
    },
    {
      "src": "/fonts/(.*(woff2?|eot))",
      "headers": { "cache-control": "public, max-age=31536000" },
      "dest": "/fonts/$1"
    }
  ]
}
